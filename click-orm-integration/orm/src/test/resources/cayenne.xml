<?xml version="1.0" encoding="utf-8"?>
<domains project-version="3.0.0.1">
<domain name="cayenneDomain">
	<map name="cayenneDomainMap" location="cayenneDomainMap.map.xml"/>

	<node name="cayenneDomainNode"
		 datasource="cayenneDomainNode.driver.xml"
		 factory="org.apache.cayenne.conf.DriverDataSourceFactory"
		 schema-update-strategy="org.apache.cayenne.access.dbsync.CreateIfNoSchemaStrategy">
			<map-ref name="cayenneDomainMap"/>
	 </node>
</domain>
</domains>
