<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping
>
    <class
        name="com.gilbertoca.gfi.model.ger.Escolaridade"
        table="ger.ger_escolaridade"
    >

        <id
            name="cdescolaridade"
            column="cdescolaridade"
            type="java.lang.Short"
        >
            <generator class="assigned">
              <!--  
                  To add non XDoclet generator parameters, create a file named 
                  hibernate-generator-params-Escolaridade.xml 
                  containing the additional parameters and place it in your merge dir. 
              --> 
            </generator>
        </id>

        <property
            name="dcescolaridade"
            type="java.lang.String"
            update="true"
            insert="true"
            column="dcescolaridade"
            length="40"
        />

        <set
            name="gerPessoacaracteristicas"
            lazy="true"
            inverse="true"
            cascade="none"
            sort="unsorted"
        >

            <key
                column="cdescolaridade"
            >
            </key>

            <one-to-many
                  class="com.gilbertoca.gfi.model.ger.PessoaCaracteristica"
            />

        </set>

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-Escolaridade.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>
