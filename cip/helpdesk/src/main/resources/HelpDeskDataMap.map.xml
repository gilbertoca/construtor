<?xml version="1.0" encoding="utf-8"?>
<data-map xmlns="http://cayenne.apache.org/schema/3.0/modelMap"
	 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	 xsi:schemaLocation="http://cayenne.apache.org/schema/3.0/modelMap http://cayenne.apache.org/schema/3.0/modelMap.xsd"
	 project-version="6">
	<property name="defaultPackage" value="helpdesk.model"/>
	<db-entity name="equipment">
		<db-attribute name="dt_acquired" type="DATE"/>
		<db-attribute name="dt_disposed" type="DATE"/>
		<db-attribute name="equipment_code" type="VARCHAR" length="20"/>
		<db-attribute name="equipment_name" type="VARCHAR" isMandatory="true" length="60"/>
		<db-attribute name="equipment_type_id" type="INTEGER" isMandatory="true"/>
		<db-attribute name="id" type="INTEGER" isPrimaryKey="true" isMandatory="true"/>
		<db-attribute name="manufacturer_name" type="VARCHAR" length="60"/>
		<db-attribute name="other_details" type="VARCHAR" length="255"/>
	</db-entity>
	<db-entity name="equipment_type">
		<db-attribute name="description" type="VARCHAR" length="50"/>
		<db-attribute name="id" type="INTEGER" isPrimaryKey="true" isMandatory="true"/>
	</db-entity>
	<db-entity name="priority_level">
		<db-attribute name="description" type="VARCHAR" isMandatory="true" length="60"/>
		<db-attribute name="priority_level" type="VARCHAR" isPrimaryKey="true" isMandatory="true" length="10"/>
	</db-entity>
	<db-entity name="problem">
		<db-attribute name="description" type="VARCHAR" length="2000"/>
		<db-attribute name="dt_reported" type="DATE" isMandatory="true"/>
		<db-attribute name="equipment_id" type="INTEGER" isMandatory="true"/>
		<db-attribute name="id" type="INTEGER" isPrimaryKey="true" isMandatory="true"/>
		<db-attribute name="user_id" type="INTEGER" isMandatory="true"/>
	</db-entity>
	<db-entity name="problem_history">
		<db-attribute name="dt_fix" type="DATE"/>
		<db-attribute name="id" type="INTEGER" isPrimaryKey="true" isMandatory="true"/>
		<db-attribute name="priority_level" type="VARCHAR" isMandatory="true" length="10"/>
		<db-attribute name="problem_id" type="INTEGER" isMandatory="true"/>
		<db-attribute name="problem_status" type="VARCHAR" isMandatory="true" length="10"/>
		<db-attribute name="support_staff_id" type="INTEGER" isMandatory="true"/>
	</db-entity>
	<db-entity name="problem_status">
		<db-attribute name="description" type="VARCHAR" isMandatory="true" length="60"/>
		<db-attribute name="problem_status" type="VARCHAR" isPrimaryKey="true" isMandatory="true" length="10"/>
	</db-entity>
	<db-entity name="resolution">
		<db-attribute name="description" type="VARCHAR" isMandatory="true" length="60"/>
		<db-attribute name="id" type="INTEGER" isPrimaryKey="true" isMandatory="true"/>
		<db-attribute name="problem_history_id" type="INTEGER" isMandatory="true"/>
	</db-entity>
	<db-entity name="skill">
		<db-attribute name="description" type="VARCHAR" isMandatory="true" length="50"/>
		<db-attribute name="id" type="INTEGER" isPrimaryKey="true" isMandatory="true"/>
	</db-entity>
	<db-entity name="staff_skill">
		<db-attribute name="dt_obtained" type="DATE" isMandatory="true"/>
		<db-attribute name="skill_id" type="INTEGER" isPrimaryKey="true" isMandatory="true"/>
		<db-attribute name="support_staff_id" type="INTEGER" isPrimaryKey="true" isMandatory="true"/>
	</db-entity>
	<db-entity name="support_staff">
		<db-attribute name="dt_joined" type="DATE" isMandatory="true"/>
		<db-attribute name="dt_left" type="DATE"/>
		<db-attribute name="email" type="VARCHAR" isMandatory="true" length="80"/>
		<db-attribute name="id" type="INTEGER" isPrimaryKey="true" isMandatory="true"/>
		<db-attribute name="location" type="VARCHAR" length="50"/>
		<db-attribute name="name" type="VARCHAR" isMandatory="true" length="60"/>
		<db-attribute name="phone" type="VARCHAR" length="30"/>
	</db-entity>
	<db-entity name="user">
		<db-attribute name="address" type="VARCHAR" length="100"/>
		<db-attribute name="email" type="VARCHAR" isMandatory="true" length="80"/>
		<db-attribute name="first_name" type="VARCHAR" isMandatory="true" length="30"/>
		<db-attribute name="id" type="INTEGER" isPrimaryKey="true" isMandatory="true"/>
		<db-attribute name="last_name" type="VARCHAR" isMandatory="true" length="60"/>
		<db-attribute name="phone" type="VARCHAR" length="30"/>
		<db-attribute name="user_type" type="VARCHAR" isMandatory="true" length="20"/>
	</db-entity>
	<db-entity name="user_type">
		<db-attribute name="description" type="VARCHAR" isMandatory="true" length="60"/>
		<db-attribute name="user_type" type="VARCHAR" isPrimaryKey="true" isMandatory="true" length="20"/>
	</db-entity>
	<obj-entity name="Equipment" className="helpdesk.model.Equipment" dbEntityName="equipment">
		<obj-attribute name="dtAcquired" type="java.util.Date" db-attribute-path="dt_acquired"/>
		<obj-attribute name="dtDisposed" type="java.util.Date" db-attribute-path="dt_disposed"/>
		<obj-attribute name="equipmentCode" type="java.lang.String" db-attribute-path="equipment_code"/>
		<obj-attribute name="equipmentName" type="java.lang.String" db-attribute-path="equipment_name"/>
		<obj-attribute name="equipmentTypeId" type="java.lang.Integer" db-attribute-path="equipment_type_id"/>
		<obj-attribute name="id" type="java.lang.Integer" db-attribute-path="id"/>
		<obj-attribute name="manufacturerName" type="java.lang.String" db-attribute-path="manufacturer_name"/>
		<obj-attribute name="otherDetails" type="java.lang.String" db-attribute-path="other_details"/>
	</obj-entity>
	<obj-entity name="EquipmentType" className="helpdesk.model.EquipmentType" dbEntityName="equipment_type">
		<obj-attribute name="description" type="java.lang.String" db-attribute-path="description"/>
		<obj-attribute name="id" type="java.lang.Integer" db-attribute-path="id"/>
	</obj-entity>
	<db-relationship name="equipmentType" source="equipment" target="equipment_type" toMany="false">
		<db-attribute-pair source="equipment_type_id" target="id"/>
	</db-relationship>
	<db-relationship name="problems" source="equipment" target="problem" toMany="true">
		<db-attribute-pair source="id" target="equipment_id"/>
	</db-relationship>
	<db-relationship name="equipments" source="equipment_type" target="equipment" toMany="true">
		<db-attribute-pair source="id" target="equipment_type_id"/>
	</db-relationship>
	<db-relationship name="equipment" source="problem" target="equipment" toMany="false">
		<db-attribute-pair source="equipment_id" target="id"/>
	</db-relationship>
	<db-relationship name="problemHistories" source="problem" target="problem_history" toMany="true">
		<db-attribute-pair source="id" target="problem_id"/>
	</db-relationship>
	<db-relationship name="user" source="problem" target="user" toMany="false">
		<db-attribute-pair source="user_id" target="id"/>
	</db-relationship>
	<db-relationship name="problem" source="problem_history" target="problem" toMany="false">
		<db-attribute-pair source="problem_id" target="id"/>
	</db-relationship>
	<db-relationship name="resolutions" source="problem_history" target="resolution" toMany="true">
		<db-attribute-pair source="id" target="problem_history_id"/>
	</db-relationship>
	<db-relationship name="supportStaff" source="problem_history" target="support_staff" toMany="false">
		<db-attribute-pair source="support_staff_id" target="id"/>
	</db-relationship>
	<db-relationship name="problemHistory" source="resolution" target="problem_history" toMany="false">
		<db-attribute-pair source="problem_history_id" target="id"/>
	</db-relationship>
	<db-relationship name="problemHistories" source="support_staff" target="problem_history" toMany="true">
		<db-attribute-pair source="id" target="support_staff_id"/>
	</db-relationship>
	<db-relationship name="problems" source="user" target="problem" toMany="true">
		<db-attribute-pair source="id" target="user_id"/>
	</db-relationship>
</data-map>
